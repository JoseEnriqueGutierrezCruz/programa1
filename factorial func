// FUNCION FACTORIAL.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
float factorial_func (float factorial, float i);

void main ()
{
	int factorial, i, n;
    factorial=1;

	cout<<"ingrese el valor de n:";
	cin>>n;

	for (i=1;i<=n;i++);

	factorial= factorial_func(factorial,i);
	cout << "el factorial de"<<n<<"es:"<<factorial;
	getch();
}

float factorial_func (float factorial, float i)
	{
	
	float FAC;
    FAC = factorial*i;
    return FAC;
